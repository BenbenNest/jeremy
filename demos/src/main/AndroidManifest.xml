<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeremy.demos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jeremy.demos.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.jeremy.demos.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jeremy.demos.activity.AIDLActivity" />

        <service
            android:name="com.jeremy.demos.service.MyAIDLService"
            android:process=":AIDLAdd" />

        <activity android:name="com.jeremy.demos.activity.YouMengActivity"></activity>
    </application>

</manifest>