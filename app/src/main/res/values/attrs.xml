<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UltimateRecyclerview">
        <attr name="recyclerviewClipToPadding" format="boolean" />
        <attr name="recyclerviewPadding" format="dimension" />
        <attr name="recyclerviewPaddingTop" format="dimension" />
        <attr name="recyclerviewPaddingBottom" format="dimension" />
        <attr name="recyclerviewPaddingLeft" format="dimension" />
        <attr name="recyclerviewPaddingRight" format="dimension" />
        <attr name="recyclerviewEmptyView" format="reference" />
        <attr name="recyclerviewFloatingActionView" format="reference" />
        <attr name="recyclerviewScrollbars" format="enum">
            <enum name="none" value="0" />
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
        <attr name="recyclerviewDefaultSwipeColor" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
            <flag name="top" value="4" />
            <flag name="bottom" value="8" />
        </attr>
        <attr name="leftEdgeSwipeOffset" format="dimension" />
        <attr name="rightEdgeSwipeOffset" format="dimension" />
        <attr name="topEdgeSwipeOffset" format="dimension" />
        <attr name="bottomEdgeSwipeOffset" format="dimension" />
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0" />
            <enum name="pull_out" value="1" />
        </attr>
        <attr name="clickToClose" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SlidingTabLayout">
        <attr name="tabTextDefaultColor" format="color" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="tabTextSelectedColor" format="color" />
        <attr name="tabTextSelectedSize" format="dimension" />
        <attr name="tabTextSelectedPadding" format="dimension" />
        <attr name="tabLayoutIndicatorColor" format="color" />
        <attr name="tabLayoutIndicatorWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="PullListRecyclerView">
        <attr name="refershBarStartOffset" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundedImageView">
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="riv_corner_radius_top_left" format="dimension" />
        <attr name="riv_corner_radius_top_right" format="dimension" />
        <attr name="riv_corner_radius_bottom_left" format="dimension" />
        <attr name="riv_corner_radius_bottom_right" format="dimension" />
        <attr name="riv_border_width" format="dimension" />
        <attr name="riv_border_color" format="color" />
        <attr name="riv_mutate_background" format="boolean" />
        <attr name="riv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CustomRatioImageView">
        <attr name="height_ratio" format="float" />
        <attr name="width_ratio" format="float" />
    </declare-styleable>
    <declare-styleable name="UserMenuItemView">
        <attr name="menuIcon" format="reference" />
        <attr name="menuTitle" format="string" />
    </declare-styleable>
    <declare-styleable name="LoginInputLayout">
        <attr name="input_name" format="string" />
        <attr name="hint" format="string" />
        <attr name="inputType">
            <enum name="text" value="0x00000001" />
            <enum name="password" value="0x00000081" />
            <enum name="phone" value="0x00000003" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ImageTextView">
        <attr name="left_height" format="dimension" />
        <attr name="left_width" format="dimension" />
        <attr name="right_height" format="dimension" />
        <attr name="right_width" format="dimension" />
        <attr name="top_height" format="dimension" />
        <attr name="top_width" format="dimension" />
        <attr name="bottom_height" format="dimension" />
        <attr name="bottom_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MultiSwipeRefreshLayout">
        <attr name="foreground" format="reference" />
    </declare-styleable>


    <declare-styleable name="RatioImageView">
        <attr name="imageRatio" format="float" />
        <attr name="imageRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RatioTextView">
        <attr name="textRatio" format="float" />
        <attr name="textRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

    <declare-styleable name="VideoRatioTextView">
        <attr name="videoRatio" format="float" />
        <attr name="coverColor" format="color" />
        <attr name="videoRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

</resources>